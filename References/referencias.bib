@article{Okasha2009,
  title = {Lifetime-Oriented Multi-Objective Optimization of Structural Maintenance Considering System Reliability, Redundancy and Life-Cycle Cost Using {{GA}}},
  author = {Okasha, Nader M. and Frangopol, Dan M.},
  year = {2009},
