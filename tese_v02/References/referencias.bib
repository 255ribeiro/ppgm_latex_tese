@article{Fang2021,
  title = {Mdtp: {{A}} Multi-Source Deep Traffic Prediction Framework over Spatio-Temporal Trajectory Data},
  author = {Fang, Ziquan and Pan, Lu and Chen, Lu and Du, Yuntao and Gao, Yunjun},
  year = {2021},
