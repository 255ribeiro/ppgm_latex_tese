@article{demelobarrosjuniorMultifractalDetrendedCrosscorrelation2022,
  title = {Multi-Fractal Detrended Cross-Correlation Heatmaps for Time Series Analysis},
  author = {De Melo Barros Junior, Paulo Roberto and Bunge, Kianny Lopes and Serravalle Reis Rodrigues, Vitor Hugo and Ferreira Santiago, Michell Thompson and Dos Santos Marinho, Euler Bentes and Lima De Jesus Silva, Jose Luis},
  year = {2022},
