  author = {Campbell, Diarmad and {de Beer}, Johannes and Mielby, Susie and {van Campenhout}, Ignace and {van der Meulen}, Michiel and Erikkson, Ingel{\"o}v and Ganerod, Guri and Lawrence, David and Bacic, Mario and Donald, Alex and Gogu, Constantin Radu and Jelenek, Jan},
