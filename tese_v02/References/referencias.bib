@article{LI2022127684,
  title = {Exploring the Dynamic Nonlinear Relationship between Crude Oil Price and Implied Volatility Indices: {{A}} New Perspective from {{MMV-MFDFA}}},
  author = {Li, Shuping and Li, Jianfeng and Lu, Xinsheng and Sun, Yihong},
  year = {2022},
