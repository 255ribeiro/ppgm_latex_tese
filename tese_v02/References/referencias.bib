@article{WATOREK2019874,
  title = {Multifractal Cross-Correlations between the World Oil and Other Financial Markets in 2012--2017},
  author = {W{\c a}torek, Marcin and Dro{\.z}d{\.z}, Stanis{\l}aw and O{\'s}wi{\c e}cimka, Pawe{\l} and Stanuszek, Marek},
