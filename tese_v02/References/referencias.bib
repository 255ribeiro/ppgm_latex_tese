@phdthesis{PEREIRA2013,
  title = {A {{Ado{\c c}{\~a}o Do Paradigma Bim Em Escrit{\'o}rios Em Escrit{\'o}rios De Salvador}}},
  author = {PEREIRA, Ana Paula Carvalho},
  year = {2013},
  abstract = {Em geral, as melhorias na projeta{\c c}{\~a}o sempre estiveram associadas {\`a} incorpora{\c c}{\~a}o de tecnologias dispon{\'i}veis em cada {\'e}poca. Atualmente, o uso massivo de sofisticadas tecnologias tem se destacado como um poderoso instrumento de transforma{\c c}{\~a}o na maioria das atividades humanas, incluindo a projeta{\c c}{\~a}o arquitet{\^o}nica. {\'E} neste cen{\'a}rio que o presente trabalho se insere, objetivando contribuir para a compreens{\~a}o e a ado{\c c}{\~a}o do paradigma BIM (Building Information Modeling) em escrit{\'o}rios de projetos de arquitetura. Para tanto, foram analisadas as dificuldades enfrentadas pelos escrit{\'o}rios de arquitetura na ado{\c c}{\~a}o das tecnologias BIM, identificando as peculiaridades em Salvador, buscando aprimorar as condi{\c c}{\~o}es de implanta{\c c}{\~a}o. Esta pesquisa fundamenta-se metodologicamente em estudos de caso interpretados qualitativamente. Foram realizadas entrevistas em seis escrit{\'o}rios de projetos de arquitetura, em uma empresa de projeto estrutural e em outra de projeto de sistemas de ar condicionado, todas localizadas e atuantesem Salvador. Como resultados s{\~a}o apontadas particularidades no uso dessa tecnologia, focadas no processo de projeto nos escrit{\'o}rios e a intera{\c c}{\~a}o com as outras disciplinas -- estrutura e sitemas de ar condicionado. A an{\'a}lise dos dados coletados permitiu que fossem identificadas dificuldades enfrentadas pelas empresas, tais como: a resist{\^e}ncia em alterar os m{\'e}todos de trabalho j{\'a} consolidados; a necessidade de mudan{\c c}a na forma de pensar o projeto; o tempo necess{\'a}rio para implanta{\c c}{\~a}o da tecnologia; a necessidade de forma{\c c}{\~a}o de m{\~a}o de obra especializada; a integra{\c c}{\~a}o com a equipe de parceiros; a falta de planejamento para implanta{\c c}{\~a}o, dentre outras. Esta pesquisa,atrav{\'e}s da an{\'a}lise dos dados apresentados e proposi{\c c}{\~o}es formadas com base nessas an{\'a}lises e em revis{\~a}o bibliogr{\'a}fica sobre o estado da arte em BIM, pretende contribuir para que a ado{\c c}{\~a}o dessas tecnologias em escrit{\'o}rios de projetos de arquitetura possa acontecer de forma segura e produza os resultados esperados.},
  school = {UFBA},
  keywords = {Building Information Modeling (BIM). Arquitetura,Construcao e Operacao (AECO). Projetacao Arquiteto,Engenharia},
  file = {D:\Zotero\storage\P5WCFUSW\2013-A_Adoo_Do_Paradigma_Bim_Em_Escritrios_Em_Escritrios_De_Salvador.pdf}
}

@phdthesis{PEREIRA2017,
