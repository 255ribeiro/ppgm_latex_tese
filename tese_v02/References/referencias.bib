@article{Querin2006,
  title = {Evolutionary Structural Optimisation Using an Additive Algorithm},
  author = {Querin, O. M. and Steven, G. P. and Xie, Y. M.},
  year = {2000},
  journal = {Finite elements in analysis and design},
  volume = {34},
  number = {3-4},
